    "@typespec/compiler": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "~24.9.1",
    "@typespec/compiler": "workspace:^",
    "concurrently": "^9.1.2",
    "prettier": "~3.6.2"
  }
