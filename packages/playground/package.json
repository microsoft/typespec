{
  "name": "@typespec/playground",
  "private": true,
  "version": "1.0.0",
  "author": "Microsoft Corporation",
  "description": "An app to play with TYPESPEC in the browser",
  "homepage": "https://microsoft.github.io/typespec",
  "readme": "https://github.com/microsoft/typespec/blob/main/README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/typespec.git"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/typespec/issues"
  },
  "keywords": [
    "typespec"
  ],
  "type": "module",
  "main": "dist/src/lib.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "clean": "rimraf ./dist ./dist-dev ./temp ./typespecContents.json",
    "build": "tsc -p . && vite build 2>&1",
    "preview": "npm run build && vite preview",
    "start": "vite",
    "watch": "vite",
    "e2e": "cross-env PW_EXPERIMENTAL_TS_ESM=1 playwright test -c e2e ",
    "e2e:headed": "cross-env PW_EXPERIMENTAL_TS_ESM=1 playwright test -c e2e --headed",
    "test": "npm run e2e 2>&1",
    "test-official": "npm run e2e 2>&1",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix"
  },
  "files": [
    "lib/*.tsp",
    "dist/**",
    "!dist/test/**"
  ],
  "dependencies": {
    "@typespec/versioning": "~0.43.0",
    "@typespec/compiler": "~0.43.0",
    "@typespec/http": "~0.43.0",
    "@typespec/rest": "~0.43.0",
    "@typespec/openapi3": "~0.43.0",
    "@typespec/openapi": "~0.43.0",
    "@typespec/html-program-viewer": "~0.43.0",
    "@emotion/react": "^11.10.4",
    "@vitejs/plugin-react": "~3.1.0",
    "es-module-shims": "~1.7.1",
    "monaco-editor": "~0.37.1",
    "prettier": "~2.8.7",
    "react-dom": "~18.2.0",
    "react": "~18.2.0",
    "vite": "^4.2.1",
    "vscode-languageserver-textdocument": "~1.0.1",
    "vscode-languageserver": "~8.1.0",
    "lzutf8": "0.6.3",
    "debounce": "~1.2.1",
    "swagger-ui": "~4.15.5",
    "swagger-ui-react": "~4.15.5",
    "swagger-client": "~3.18.5",
    "@fluentui/react-components": "~9.18.8",
    "@fluentui/react-icons": "~2.0.190",
    "recoil": "~0.7.6"
  },
  "devDependencies": {
    "@types/mocha": "~10.0.0",
    "@types/node": "~18.11.9",
    "@types/prettier": "2.6.0",
    "@types/react-dom": "~18.0.1",
    "@types/react": "~18.0.5",
    "@typespec/eslint-config-typespec": "~0.6.0",
    "@typespec/bundler": "~0.1.0",
    "@babel/core": "^7.0.0",
    "eslint": "^8.36.0",
    "mocha": "~10.2.0",
    "mocha-junit-reporter": "~2.2.0",
    "mocha-multi-reporters": "~1.5.1",
    "c8": "~7.13.0",
    "@playwright/test": "~1.32.2",
    "rimraf": "~5.0.0",
    "cross-env": "~7.0.3",
    "typescript": "~5.0.2",
    "@types/debounce": "~1.2.1",
    "@types/swagger-ui": "~3.52.0",
    "@types/swagger-ui-react": "^4.11.0",
    "rollup-plugin-visualizer": "~5.9.0"
  }
}
