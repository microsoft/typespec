{
  "name": "@typespec/efv2-zod-sketch",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "main": "dist/src/index.js",
  "scripts": {
    "build-src": "babel src -d dist/src --extensions '.ts,.tsx'",
    "build": "tsc -p . && npm run build-src",
    "watch-src": "babel src -d dist/src --extensions '.ts,.tsx' --watch",
    "watch-tsc": "tsc -p . --watch",
    "watch": "concurrently --kill-others \"npm run watch-tsc\" \"npm run watch-src\"",
    "test": "vitest run",
    "build-todo": "tsp compile sample/main.tsp --emit @typespec/efv2-zod-sketch --output-dir sample/output/todo && babel sample/output/todo -d dist/sample/output/todo --extensions '.ts,.tsx' && npx prettier sample/output --write && npm install --prefix sample/output/todo/@typespec/efv2-zod-sketch",
    "build-petstore": "tsp compile ../samples/specs/petstore/petstore.tsp --emit @typespec/efv2-zod-sketch --output-dir sample/output/petstore && babel sample/output/petstore -d dist/sample/output/petstore --extensions '.ts,.tsx' && npx prettier sample/output --write",
    "format": "prettier . --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "peerDependencies": {
    "@typespec/compiler": "workspace:*",
    "@typespec/http-client-library": "workspace:*",
    "@typespec/http": "workspace:*",
    "@typespec/openapi": "workspace:*",
    "@typespec/rest": "workspace:*",
    "@typespec/emitter-framework": "workspace:*",
    "@typespec/json-schema": "workspace:*",
    "@typespec/openapi3": "workspace:*"
  },
  "dependencies": {
    "@alloy-js/core": "https://pkg.pr.new/alloy-framework/alloy/@alloy-js/core@0924029",
    "@alloy-js/typescript": "https://pkg.pr.new/alloy-framework/alloy/@alloy-js/typescript@0924029"
  },
  "devDependencies": {
    "@alloy-js/prettier-plugin-alloy": "latest",
    "@azure-tools/cadl-ranch-expect": "0.15.4",
    "@azure-tools/cadl-ranch-specs": "0.37.2",
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.9",
    "@rollup/plugin-babel": "^6.0.4",
    "@alloy-js/babel-preset": "https://pkg.pr.new/alloy-framework/alloy/@alloy-js/babel-preset@0924029",
    "concurrently": "^8.2.2",
    "ts-morph": "^23.0.0",
    "typescript": "^5.5.3",
    "vitest": "^2.0.3"
  }
}
