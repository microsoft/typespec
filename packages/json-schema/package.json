{
  "name": "@cadl-lang/json-schema",
  "version": "0.1.0",
  "author": "Microsoft Corporation",
  "description": "Cadl library for emitting Cadl to JSON Schema and converting JSON Schema to Cadl",
  "homepage": "https://github.com/Microsoft/cadl",
  "readme": "https://github.com/Microsoft/cadl/blob/master/README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/cadl.git"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/cadl/issues"
  },
  "keywords": [
    "cadl",
    "json schema"
  ],
  "type": "module",
  "main": "dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js"
  },
  "cadlMain": "dist/src/index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "clean": "rimraf ./dist ./temp",
    "build": "tsc -p . && npm run lint-cadl-library",
    "watch": "tsc -p . --watch",
    "lint-cadl-library": "cadl compile . --warn-as-error --import @cadl-lang/library-linter --no-emit",
    "test": "mocha",
    "test-official": "c8 mocha --forbid-only --reporter mocha-multi-reporters",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "lint:fix": "eslint . --fix --ext .ts"
  },
  "files": [
    "lib/*.cadl",
    "dist/**",
    "!dist/test/**"
  ],
  "peerDependencies": {
    "@cadl-lang/compiler": "~0.39.0"
  },
  "devDependencies": {
    "@types/mocha": "~10.0.0",
    "@types/node": "~18.11.9",
    "@cadl-lang/compiler": "~0.39.0",
    "@cadl-lang/eslint-config-cadl": "~0.5.0",
    "@cadl-lang/library-linter": "~0.39.0",
    "@cadl-lang/eslint-plugin": "~0.39.0",
    "eslint": "^8.12.0",
    "mocha": "~10.1.0",
    "mocha-junit-reporter": "~2.2.0",
    "mocha-multi-reporters": "~1.5.1",
    "c8": "~7.12.0",
    "rimraf": "~3.0.2",
    "typescript": "~4.9.3",
    "ajv": "~8.11.2",
    "@cadl-lang/openapi3": "~0.39.0",
    "@types/relateurl": "~0.2.29",
    "@types/js-yaml": "~4.0.1"
  },
  "dependencies": {
    "relateurl": "~0.2.7",
    "js-yaml": "~4.1.0"
  }
}
