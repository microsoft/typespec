<div class="navbar">
  <div class="navbar-logo">
    <a href="{{ "/"| url }}">
      <h1>Cadl</h1>
    </a>
  </div>
  <div class="navbar-links">
    <ul>
      {% for item in navbar.items %}
        <li>
          <a href="{{ item.url | url }}">{{ item.name }}</a>
        </li>
      {% endfor %}
      <li>
        <a class="icon" href="{{ metadata.githubUrl }}" target="_blank" title="Github">
          {% icon "github" %}</a>
      </li>
    </ul>
  </div>
