  "components": {
    "schemas": {
      "Fish": {
        "type": "object",
        "required": [
          "age",
          "kind"
        ],
        "properties": {
          "age": {
            "type": "integer",
            "format": "int32"
          },
          "kind": {
            "type": "string",
            "description": "Discriminator property for Fish."
          }
        },
        "discriminator": {
          "propertyName": "kind",
          "mapping": {
            "shark": "#/components/schemas/Shark",
            "salmon": "#/components/schemas/Salmon"
          }
        },
        "description": "This is base model for polymorphic multiple levels inheritance with a discriminator."
      },
      "GoblinShark": {
        "type": "object",
        "required": [
          "sharktype"
        ],
        "properties": {
          "sharktype": {
            "type": "string",
            "enum": [
              "goblin"
            ]
          }
        },
        "allOf": [
          {
            "$ref": "#/components/schemas/Shark"
          }
        ],
        "description": "The third level model GoblinShark in polymorphic multiple levels inheritance."
      },
      "Salmon": {
        "type": "object",
        "required": [
          "kind"
        ],
        "properties": {
          "kind": {
            "type": "string",
            "enum": [
              "salmon"
            ]
          },
          "friends": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Fish"
            }
          },
          "hate": {
            "type": "object",
            "additionalProperties": {
              "$ref": "#/components/schemas/Fish"
            }
          },
          "partner": {
            "$ref": "#/components/schemas/Fish"
          }
        },
        "allOf": [
          {
            "$ref": "#/components/schemas/Fish"
          }
        ],
        "description": "The second level model in polymorphic multiple levels inheritance which contains references to other polymorphic instances."
      },
      "SawShark": {
        "type": "object",
        "required": [
          "sharktype"
        ],
        "properties": {
          "sharktype": {
            "type": "string",
            "enum": [
              "saw"
            ]
          }
        },
        "allOf": [
          {
            "$ref": "#/components/schemas/Shark"
          }
        ],
        "description": "The third level model SawShark in polymorphic multiple levels inheritance."
      },
      "Shark": {
        "type": "object",
        "required": [
          "kind",
          "sharktype"
        ],
        "properties": {
          "kind": {
            "type": "string",
            "enum": [
              "shark"
            ]
          },
          "sharktype": {
            "type": "string"
          }
        },
        "discriminator": {
          "propertyName": "sharktype",
          "mapping": {
            "saw": "#/components/schemas/SawShark",
            "goblin": "#/components/schemas/GoblinShark"
          }
        },
        "allOf": [
          {
            "$ref": "#/components/schemas/Fish"
          }
        ],
        "description": "The second level model in polymorphic multiple levels inheritance and it defines a new discriminator."
      }
    }
  },
  "servers": [
    {
      "url": "http://localhost:3000",
