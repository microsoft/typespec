{
  "openapi": "3.0.0",
  "info": {
    "title": "(title)",
    "description": "Illustrates versioned server.",
    "version": "2021-01-01-preview"
  },
  "tags": [],
  "paths": {
    "/server/versions/versioned/with-path-api-version/{apiVersion}": {
      "head": {
        "operationId": "withPathApiVersion",
        "parameters": [
          {
            "name": "apiVersion",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "The request has succeeded."
          }
        }
      }
    },
    "/server/versions/versioned/with-query-api-version": {
      "head": {
        "operationId": "withQueryApiVersion",
        "parameters": [
          {
            "name": "api-version",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "explode": false
          }
        ],
        "responses": {
          "200": {
            "description": "The request has succeeded."
          }
        }
      }
    },
    "/server/versions/versioned/with-query-old-api-version": {
      "head": {
        "operationId": "withQueryOldApiVersion",
        "parameters": [
          {
            "name": "api-version",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "explode": false
          }
        ],
        "responses": {
          "200": {
            "description": "The request has succeeded."
          }
        }
      }
    },
    "/server/versions/versioned/without-api-version": {
      "head": {
        "operationId": "withoutApiVersion",
        "parameters": [],
        "responses": {
          "200": {
            "description": "The request has succeeded."
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Versions": {
        "type": "string",
        "enum": [
          "2021-01-01-preview",
          "2022-12-01-preview"
        ],
        "description": "The version of the API."
      }
    }
  },
  "servers": [
    {
      "url": "{endpoint}",
      "description": "Testserver endpoint",
      "variables": {
        "endpoint": {
          "default": "",
          "description": "Need to be set as 'http://localhost:3000' in client."
        }
      }
    }
  ]
}
