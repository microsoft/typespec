{
  "name": "@typespec/emitter-framework",
  "version": "0.8.0",
  "type": "module",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoft/typespec.git"
  },
  "scripts": {
    "build": "alloy build",
    "clean": "rimraf ./dist",
    "format": "prettier . --write",
    "watch": "alloy build --watch",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest -w",
    "test:ci": "vitest run --coverage --reporter=junit --reporter=default",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix"
  },
  "exports": {
    ".": {
      "import": "./dist/src/core/index.js"
    },
    "./csharp": {
      "import": "./dist/src/csharp/index.js"
    },
    "./typescript": {
      "import": "./dist/src/typescript/index.js"
    },
    "./testing": {
      "import": "./dist/src/testing/index.js"
    }
  },
  "imports": {
    "#test/*": "./test/*",
    "#core/*": {
      "development": "./src/core/*",
      "default": "./dist/src/core/*"
    },
    "#csharp/*": {
      "development": "./src/csharp/*",
      "default": "./dist/src/csharp/*"
    },
    "#typescript/*": {
      "development": "./src/typescript/*",
      "default": "./dist/src/typescript/*"
    }
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "peerDependencies": {
    "@alloy-js/core": "^0.18.2",
    "@alloy-js/typescript": "^0.18.0",
    "@alloy-js/csharp": "^0.18.0",
    "@typespec/compiler": "workspace:^",
    "@typespec/http": "workspace:^",
    "@typespec/rest": "workspace:^"
  },
  "devDependencies": {
    "@alloy-js/cli": "^0.18.0",
    "@alloy-js/rollup-plugin": "^0.1.0",
    "@alloy-js/core": "^0.18.2",
    "@alloy-js/typescript": "^0.18.0",
    "@typespec/compiler": "workspace:^",
    "@typespec/http": "workspace:^",
    "@typespec/rest": "workspace:^",
    "concurrently": "^9.1.2",
    "pathe": "^2.0.3",
    "prettier": "~3.5.3",
    "web-tree-sitter": "^0.25.4",
    "tree-sitter-c-sharp": "^0.23.0",
    "tree-sitter-java": "^0.23.2",
    "tree-sitter-javascript": "^0.23.0",
    "tree-sitter-python": "^0.23.2",
    "tree-sitter-typescript": "^0.23.0",
    "typescript": "~5.8.2",
    "vitest": "^3.1.2"
  }
}
