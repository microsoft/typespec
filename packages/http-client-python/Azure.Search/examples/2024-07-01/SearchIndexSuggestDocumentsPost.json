{
  "operationId": "DocumentsOperations_SuggestPost",
  "title": "SearchIndexSuggestDocumentsPost",
  "parameters": {
    "endpoint": "https://myservice.search.windows.net",
    "indexName": "myindex",
    "api-version": "2024-07-01",
    "suggestRequest": {
      "filter": "rating gt 4.0",
      "highlightPostTag": "</em>",
      "highlightPreTag": "<em>",
      "minimumCoverage": 80,
      "orderby": "rating desc",
      "search": "hote",
      "searchFields": "title",
      "select": "docId,title,description",
      "suggesterName": "sg",
      "top": 10
    }
  },
  "responses": {
    "200": {
      "body": {
        "value": [
          {
            "@search.text": "Nice <em>Hotel</em>",
            "description": "Cheapest hotel in town",
            "docId": "1",
            "title": "Nice Hotel"
          },
          {
            "@search.text": "Fancy <em>Hotel</em>",
            "description": "Best hotel in town",
            "docId": "2",
            "title": "Fancy Hotel"
          }
        ]
      }
    }
  }
}
