            "dataSourceName": "mydocdbdatasource",
            "targetIndexName": "orders",
            "schedule": {
              "interval": "PT1H",
              "startTime": "2015-01-01T00:00:00Z"
            },
            "parameters": {
              "maxFailedItems": 10,
              "maxFailedItemsPerBatch": 5
            },
            "fieldMappings": [],
            "disabled": false
          },
          {
            "name": "myotherindexer",
