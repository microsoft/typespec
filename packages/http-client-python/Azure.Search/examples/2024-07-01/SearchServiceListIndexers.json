{
  "operationId": "IndexersOperations_List",
  "title": "SearchServiceListIndexers",
  "parameters": {
    "endpoint": "https://myservice.search.windows.net",
    "$select": "*",
    "api-version": "2024-07-01"
  },
  "responses": {
    "200": {
      "body": {
        "value": [
          {
            "name": "myindexer",
