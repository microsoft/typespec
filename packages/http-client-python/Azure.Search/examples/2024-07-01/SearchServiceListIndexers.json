            "targetIndexName": "orders",
            "parameters": {
              "maxFailedItems": 10,
              "maxFailedItemsPerBatch": 5,
              "batchSize": 15
            },
            "fieldMappings": [
              {
                "sourceFieldName": "PersonName",
                "targetFieldName": "FirstName",
                "mappingFunction": {
                  "name": "extractTokenAtPosition",
