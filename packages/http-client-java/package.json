  },
  "license": "MIT",
  "type": "module",
  "main": "emitter/dist/src/emitter.js",
  "exports": {
    ".": "./emitter/dist/src/emitter.js",
    "./testing": "./dist/src/testing/index.js"
  },
  "tspMain": "emitter/dist/src/emitter.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
