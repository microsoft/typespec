{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "./dist/emitter/index.d.ts",
  "projectFolder": "./emitter",
  "compiler": {
    "tsconfigFilePath": "./emitter/tsconfig.build.json"
  },
  "apiReport": {
    "enabled": true,
    "reportFolder": "./emitter/temp",
    "reportFileName": "typespec-http-client-csharp.api.md"
  },
  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "./emitter/temp/typespec-http-client-csharp.api.json"
  },
  "dtsRollup": {
    "enabled": true,
    "publicTrimmedFilePath": "./dist/typespec-http-client-csharp.d.ts",
    "untrimmedFilePath": ""
  },
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    }
  }
}
